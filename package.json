{
  "name": "fastify-api-template",
  "version": "1.0.0",
  "description": "Modern, modular Fastify API template with JWT authentication, PostgreSQL, and Prisma ORM",
  "main": "src/server.js",
  "scripts": {
    "postinstall": "prisma generate",
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "db:migrate": "prisma migrate dev",
    "db:seed": "node prisma/seeds/seed.js",
    "db:fix": "node fix_migration.js"
  },
  "keywords": [
    "fastify",
    "api",
    "template",
    "prisma",
    "jwt",
    "postgresql"
  ],
  "author": "anon",
  "license": "MIT",
  "dependencies": {
    "@fastify/aws-lambda": "^5.1.4",
    "@fastify/cors": "^11.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.2",
    "@logtail/pino": "^0.5.2",
    "@prisma/client": "^6.6.0",
    "bcrypt": "^5.1.1",
    "busboy": "^1.6.0",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.2",
    "fastify-plugin": "^4.5.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.5.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.7.0",
    "nodemon": "^3.1.0"
  }
}
